<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.xumaodun.a_fastdeveop">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />
    <uses-permission android:name="android.permission.READ_PHONE_STATE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:name=".common.AppContext"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".splash.SplashActivity" />
        <activity android:name=".splash.SplashOneActivity" />
        <activity android:name=".tabmain.TabMainActivity" />
        <activity
            android:name=".splash.SplashTwoActivity"
            android:label="@string/app_intro" />
        <activity android:name=".vedio.VedioActivity" />
        <activity android:name=".selector.SelectorActivity" />
        <activity android:name=".viewpager.ViewPagerAvtivity" />
        <activity android:name=".refresh.RefreshActivity" />
        <activity android:name=".refresh.MaterialActivity" />
        <activity android:name=".refresh.pull.PullToRefreshActivity" />
        <activity android:name=".refresh.ScrollViewActivity" />
        <activity android:name=".refresh.RecyclerViewActivity" />
        <activity android:name=".refresh.WaveViewActivity" />
        <activity android:name=".refresh.LoadMoreViewActivity" />
        <activity android:name=".picture.PictureActivity" />
        <activity android:name=".picture.imageloader.ImageLoaderActivity" />
        <activity android:name=".picture.CircleImageViewActivity" />
        <activity android:name=".refresh.pull.GridViewActivity" />
        <activity android:name=".picture.glide.GlideActivity" />
        <activity android:name=".retrofit.RetrofitActivity"></activity>
    </application>

</manifest>